{
  "name": "@whitespace/cookie-consent",
  "version": "2.1.0",
  "main": "dist/cookie-consent.min.js",
  "module": "lib/index.js",
  "author": "Whitespace <code@whitespace.se>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -rf lib",
    "build": "yarn build:umd && yarn build:node",
    "build:umd": "parcel build 'src/index.js' --out-file dist/cookie-consent.min.js --global CookieConsent",
    "build:node": "parcel build 'src/**/*' --out-dir lib --target node --no-minify --no-source-maps",
    "prepare": "yarn clean && yarn build",
    "dev": "yarn dev:umd & yarn dev:node",
    "dev:umd": "parcel watch 'src/index.js' --out-file dist/cookie-consent.min.js --global CookieConsent",
    "dev:node": "parcel watch 'src/**/*' --out-dir lib --target node --no-source-maps",
    "start": "yarn dev",
    "format": "prettier --write '**/*.{js,json,html,md,yml,scss,css}'",
    "test": "serve ."
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "sass": "^1.26.8",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yett": "^0.1.13"
  },
  "browserslist": [
    "defaults"
  ]
}
